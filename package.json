{
	"name": "@kingbain/status-page",
	"private": false,
	"version": "0.0.17",
	"type": "module",
	"repository": "https://github.com/KingBain/new-status-page",
	"publishConfig": {
		"registry": "https://npm.pkg.github.com"
	},
	"scripts": {
		"pre-process": "npx tsx pre-process.ts",
		"post-process": "npx tsx post-process.ts",
		"dev": "vite dev",
		"build": "vite build",
		"export": "npm run pre-process && npm run post-process",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"cy:run": "npx cypress run",
		"cy:open": "npx cypress open",
		"test": "npm run init-tests && run-p --race dev cy:run",
		"init-tests": "npx tsx init-tests.ts"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@sveltejs/adapter-auto": "^4.0.0",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@types/node": "^22.13.10",
		"@types/rollup": "^0.51.4",
		"cypress": "^14.2.0",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"rollup": "^4.36.0",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^6.3.5"
	},
	"dependencies": {
		"@octokit/rest": "^21.1.1",
		"@sveltejs/adapter-static": "^3.0.8",
		"chart.js": "^4.4.8",
		"npm-run-all": "^4.1.5",
		"snarkdown": "^2.0.0",
		"ts-node": "^10.9.2"
	}
}
