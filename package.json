{
	"name": "@kingbain/status-page-sveltekit",
	"private": false,
	"version": "0.0.17",
	"publishConfig": {
		"access": "public",
		"provenance": true
	},
	"type": "module",
	"repository": "https://github.com/KingBain/status-page-sveltekit",
	"scripts": {
		"pre-process": "npx tsx pre-process.ts",
		"post-process": "npx tsx post-process.ts",
		"dev": "vite dev",
		"build": "vite build",
		"export": "npm run pre-process && npm run post-process",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"cy:run": "npx cypress run",
		"cy:open": "npx cypress open",
		"test": "npm run init-tests && run-p --race dev cy:run",
		"init-tests": "npx tsx init-tests.ts"
	},
	"devDependencies": {
		"@eslint/compat": "^2.0.2",
		"@eslint/js": "^10.0.1",
		"@semantic-release/commit-analyzer": "^13.0.1",
		"@semantic-release/github": "^12.0.6",
		"@semantic-release/npm": "^13.1.4",
		"@semantic-release/release-notes-generator": "^14.0.3",
		"@sveltejs/adapter-auto": "^7.0.1",
		"@sveltejs/kit": "^2.53.2",
		"@sveltejs/vite-plugin-svelte": "^6.2.4",
		"@types/node": "^25.3.1",
		"@types/rollup": "^0.54.0",
		"cypress": "^15.11.0",
		"eslint": "^10.0.2",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.15.0",
		"globals": "^17.3.0",
		"prettier": "^3.8.1",
		"prettier-plugin-svelte": "^3.5.0",
		"rollup": "^4.59.0",
		"semantic-release": "^25.0.3",
		"svelte": "^5.53.5",
		"svelte-check": "^4.4.3",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.56.1",
		"vite": "^7.3.1"
	},
	"dependencies": {
		"@octokit/rest": "^22.0.1",
		"@sveltejs/adapter-static": "^3.0.10",
		"chart.js": "^4.5.1",
		"npm-run-all": "^4.1.5",
		"snarkdown": "^2.0.0",
		"ts-node": "^10.9.2"
	}
}
